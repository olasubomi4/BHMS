@model IEnumerable<BHMS.CORE.Models.ObjectDetector>

@{
    ViewBag.Title = "Insights from video";
}

<h2>Detected</h2>

<p>
    @Html.ActionLink("Go back", "Index")
</p>
@{
    if (ViewBag.missing == "MISSING")
    {

        <h2> Object has not been investigated</h2>
    }

    else
    {
        foreach (var item in Model)
        {
            <h2> your item  @Html.DisplayFor(modelItem => item.name) was dectected from   @Html.DisplayFor(modelItem => item.startTime) to   @Html.DisplayFor(modelItem => item.endTime) with a confidence of  @Html.DisplayFor(modelItem => item.confidence)</h2>
            <iframe width="420" height="315"
                    src="@Html.Raw(ViewBag.linkk)">
            </iframe>




        }
    }
}



