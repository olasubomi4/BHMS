
@model IEnumerable<BHMS.CORE.Models.ObjectDetector>
@{
    ViewBag.Title = "ObjectDetector";
}
@{ if (ViewBag.ok == "OK")
    {
        <p>your item  @ViewBag.name was dectected from    @ViewBag.startTime to @ViewBag.endTime with a confidence of  @ViewBag.confidence</p>
        <div class="row">
            <div class="col-sm-12">
                <iframe *ngIf=@ViewBag.playerWidgetLink style="width: 100%; height:900px;" [src]-@ViewBag.playerWidgetLink frameborder-"O" allowfullscreen></iframe>
            </div>
        </div>
        <iframe width="420" height="315"
                src="@Html.Raw(ViewBag.playerWidgetLin)">
        </iframe>
        <iframe width="420" height="315"
                src="@Html.Raw(@ViewBag.playerWidgetLin)">
        </iframe>
        <iframe width="420" height="315"
                src="@Html.Raw(@ViewBag.K)">
        </iframe>
        <iframe width="420" height="315"
                src="@Html.Raw(ViewBag.K)">
        </iframe>


    }

    else
    {
        <p>Object does not exist </p>
    }
}
<h2>ObjectDetector</h2>


